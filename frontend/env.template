# Frontend Environment Variables
# Copy this to .env.local and fill in your values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_ENVIRONMENT=development

# Map Configuration (for Melbourne parking data)
REACT_APP_MAPBOX_TOKEN=your_mapbox_token_here
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Melbourne Open Data API
REACT_APP_MELBOURNE_DATA_API_KEY=your_melbourne_data_api_key_here
REACT_APP_MELBOURNE_PARKING_API_URL=https://data.melbourne.vic.gov.au/api

# Third-party Services
REACT_APP_ANALYTICS_ID=your_analytics_id_here

# Feature Flags
REACT_APP_ENABLE_REAL_TIME_UPDATES=true
REACT_APP_ENABLE_PARKING_PREDICTIONS=true