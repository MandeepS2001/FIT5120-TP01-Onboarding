# Data Science Environment Variables
# Copy this to .env and fill in your values

# API Configuration
API_BASE_URL=http://localhost:5000/api

# Melbourne Data Sources
MELBOURNE_DATA_API_KEY=your_melbourne_data_api_key_here
MELBOURNE_PARKING_API_URL=https://data.melbourne.vic.gov.au/api
VICROADS_API_KEY=your_vicroads_api_key_here

# Google APIs
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_S3_BUCKET=melbourne-parking-ml-data

# Database Configuration (MySQL)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_DB=melbourne_parking
MYSQL_USER=parking_user
MYSQL_PASSWORD=strong_password

# Machine Learning Configuration
MODEL_ARTIFACTS_PATH=./models
DATA_PATH=./data
EXPERIMENT_TRACKING_URI=./mlruns

# Jupyter Configuration
JUPYTER_PORT=8888
JUPYTER_TOKEN=your_jupyter_token_here

# Feature Engineering
TIME_WINDOW_MINUTES=30
PREDICTION_HORIZON_HOURS=2
FEATURE_STORE_PATH=./data/features

# Model Training
TRAIN_SPLIT_RATIO=0.8
VALIDATION_SPLIT_RATIO=0.1
TEST_SPLIT_RATIO=0.1
RANDOM_SEED=42

# Real-time Processing
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
REDIS_HOST=localhost
REDIS_PORT=6379

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/ml_pipeline.log